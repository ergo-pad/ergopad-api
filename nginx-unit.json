{
    "listeners": {
        "*:8000": {
            "pass": "applications/fastapi"
        }
    },

    "applications": {
        "fastapi": {
            "type": "python 3.10",
            "processes": 4,
            "path": "/app",
            "home": "/usr/local",
            "module": "main",
            "callable": "app"
        }
    }
}